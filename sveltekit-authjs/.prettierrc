{
    "useTabs": false,
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "printWidth": 100,
    "endOfLine": "lf",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "singleAttributePerLine": true,
    "plugins": [
        "prettier-plugin-svelte"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ]
}